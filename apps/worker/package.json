{
  "name": "@ttpos/a-app-worker",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.7.1",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "cf:typegen": "wrangler types ./server/types/worker-configuration.d.ts",
    "cf:localdb": "wrangler d1 migrations apply a-app-url-short",
    "cf:deploy": "bash ./devfiles/cf-deploy.sh",
    "cf:dev": "wrangler dev --ip 0.0.0.0 --port 3000 .output/server/index.mjs --site .output/public",
    "db:studio": "drizzle-kit studio --port 3003 --host 0.0.0.0",
    "db:pull": "drizzle-kit pull",
    "db:gen": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:up": "drizzle-kit up",
    "lint": "eslint ."
  },
  "dependencies": {
    "@libsql/client": "^0.10.0",
    "drizzle-orm": "^0.33.0",
    "jose": "^5.8.0",
    "nuxt": "^3.13.0",
    "tracer": "^1.3.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240821.1",
    "@nuxt/eslint-config": "^0.5.3",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.9.0",
    "nitro-cloudflare-dev": "^0.1.6",
    "vue-tsc": "latest",
    "wrangler": "^3.72.3"
  }
}
