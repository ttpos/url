# Docker images name
POD_NAME=
# Production license for @nuxt/ui-pro, get one at https://ui.nuxt.com/pro/purchase
NUXT_UI_PRO_LICENSE=
# Public URL, used for OG Image when running nuxt generate
NUXT_PUBLIC_SITE_URL=

# LibSQL Configuration
# The URL for connecting to the LibSQL database. Default is a local SQLite file.
NUXT_LIBSQL_URL=file:server/database/data.db

# The authentication token for accessing the LibSQL database.
NUXT_LIBSQL_AUTH_TOKEN=

NUXT_GMAIL_EMAIL_USER=**********
NUXT_GMAIL_EMAIL_PASSWORD=**********

NUXT_OAUTH_GITHUB_CLIENT_ID=**********
NUXT_OAUTH_GITHUB_CLIENT_SECRET=**********

NUXT_OAUTH_GOOGLE_CLIENT_ID=**********
NUXT_OAUTH_GOOGLE_CLIENT_SECRET=**********
NUXT_OAUTH_GOOGLE_REDIRECT_URI=**********
