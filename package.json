{
  "name": "@ttpos/url-monorepo",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "description": "A simple and efficient URL shortener",
  "repository": {
    "type": "git",
    "url": "https://github.com/ttpos/url.git"
  },
  "engines": {
    "node": ">=18.20.3 || >=20.0.0"
  },
  "scripts": {
    "dev:links": "pnpm -C apps/links dev",
    "dev:dashboard": "pnpm -C apps/dashboard dev",
    "preview:links": "pnpm -C apps/links preview",
    "preview:dashboard": "pnpm -C apps/dashboard preview",
    "build": "pnpm -r build",
    "clean": "bash ./scripts/clean.sh && pnpm -r clean",
    "prepare": "bash ./scripts/init.sh",
    "release": "pnpm publish -r --filter @ttpos/* --no-git-checks --tag=latest",
    "release:beta": "pnpm publish -r --filter @ttpos/* --no-git-checks --tag=beta",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.5.0",
    "@types/node": "^22.2.0",
    "eslint": "^9.10.0",
    "typescript": "^5.5.4"
  }
}
